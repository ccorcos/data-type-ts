{
	"name": "data-type-ts",
	"version": "0.0.2",
	"description": "Durable data types for TypeScript with runtime validators.",
	"main": "build/src/index.js",
	"scripts": {
		"build": "tsc",
		"watch": "tsc --watch",
		"test": "tsc && ava build/test/**/*.js -v",
		"release": "rm -rf build && npm run build && npm version patch && npm publish"
	},
	"keywords": [
		"runtime",
		"schema",
		"jsonschema",
		"durable",
		"type",
		"validate",
		"validation",
		"validator",
		"datatype",
		"typescript"
	],
	"author": "Chet Corcos <ccorcos@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.149",
		"@types/node": "^13.1.8",
		"ava": "^3.0.0-beta.1",
		"typescript": "^3.7.5"
	},
	"files": [
		"build/src/*"
	]
}
